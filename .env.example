# Discord
DISCORD_TOKEN=

# Platform: "google" or "openai"
PLATFORM=google

# Google Gemini API (when PLATFORM=google)
GOOGLE_API_KEY=
MODEL=gemini-2.0-flash-exp

# OpenAI-compatible API (when PLATFORM=openai)
# For OpenAI, OpenRouter, or other compatible providers
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
# MODEL=gpt-4o
# For OpenRouter: OPENAI_BASE_URL=https://openrouter.ai/api/v1

# Context settings
CHANNEL_HISTORY_LIMIT=50
REPLY_CHAIN_LIMIT=50

# Web search (Tavily)
ENABLE_WEB_SEARCH=true
SEARCH_PROVIDER=tavily
TAVILY_API_KEY=
